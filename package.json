    "@yarnpkg/plugin-pnp": "^2.2.1",
    "clipanion": "^2.4.4",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "codecov": "^3.7.2",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
